<main id="delete_meet">
    <div class="titleinfo">
        <center>
            <h2>Delete Dog Meet</h2>
            <p>
                Please input the dog meet's name you wish to delete.
            </p>
        </center>
    </div>

    <div class="ownerinfo">
        <input type="text" id="meetName" class="input" name="meetName" placeholder="Dog Meet Name" style="margin-top: 5%">
        <button class="btn" name="button1" style="margin-top: 5%" onclick="deleteMeet()">Delete Dog Meet</button>
    </div>
</main>

<script>
    function deleteMeet() {
        var name = document.getElementById("meetName").value;
        window.location = "/dog_meets/delete_meet/" + encodeURI(name);
    }
</script>
